{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "academe/omnipay-authorizenetapi": "^3.1",
        "anandsiddharth/laravel-paytm-wallet": "^2.0",
        "anhskohbo/no-captcha": "^3.6",
        "barryvdh/laravel-dompdf": "^2.1",
        "baselrabia/myfatoorah-with-laravel": "^1.0",
        "cartalyst/stripe-laravel": "^16.0",
        "google/apiclient": "^2.15.0",
        "guzzlehttp/guzzle": "^7.8",
        "kreait/firebase-php": "^7.16",
        "kreativdev/installer": "^1.1",
        "laravel-notification-channels/webpush": "^8.0",
        "laravel/framework": "^11.48",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.11",
        "laravel/tinker": "^2.9",
        "maatwebsite/excel": "^3.1",
        "mews/purifier": "^3.4",
        "mollie/laravel-mollie": "^3.0",
        "omnipay/common": "^3.3",
        "php-http/guzzle7-adapter": "^1.0",
        "phpmailer/phpmailer": "^6.4",
        "razorpay/razorpay": "2.*",
        "spatie/laravel-cookie-consent": "^3.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
